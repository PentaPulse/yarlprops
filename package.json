{
  "name": "yarlprops",
  "version": "0.0.12",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "devextreme-react": "^24.1.5",
    "firebase": "^10.13.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "react-storage-complete": "^1.1.10",
    "web-vitals": "^4.2.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "testdb": "node ./src/api/db/test.js",
    "eject": "react-scripts eject",
    "start-emulators": "bash ./run_emulators.sh start",
    "emu-charith": "firebase emulators:start --import=./datacharith",
    "export-charith": "firebase emulators:export ./datacharith",
    "emu-janani": "firebase emulators:start --import=./datajanani",
    "export-janani": "firebase emulators:export ./datajanani",
    "emu-priya": "firebase emulators:start --import=./datapriya",
    "export-priya": "firebase emulators:export ./datapriya",
    "emu-mith": "firebase emulators:start --import=./datamith",
    "export-mith": "firebase emulators:export ./datamith",
    "emu-sachi": "firebase emulators:start --import=./datas",
    "export-sachi": "firebase emulators:export ./datas",
    "build:main": "cd main-app && npm run build && cp -r build/* ../main",
    "build:admin": "cd admin-app && npm run build && cp -r build/* ../admin",
    "start:main": "cd main-app && npm start",
    "start:admin": "cd admin-app && npm start"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/leaflet": "^1.9.12"
  }
}
