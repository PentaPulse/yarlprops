rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    
    match /systemusers/{id}{
    allow read,write;
    }
    
    match /contactUs/{id}{
    allow read;
    allow write;
    }

		match /products/{productId}{
    	allow read;
      allow write;
    }
		match /services/{serviceId}{
    	allow read;
      allow write;
    }
  }
}